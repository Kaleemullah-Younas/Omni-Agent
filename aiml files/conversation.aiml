    <category>
        <pattern>I AM *</pattern>
        <template>
            <think><set name="mood"><star/></set></think>
            <condition name="sentiment">
                <li value="positive">
                    Happy to know. You are in a great mood.
                </li>
                <li value="negative">
                    Do not be negative. Here is a joke to lighten your mood.<br/>
                    <srai>TELL ME A JOKE</srai>
                </li>
                <li value="">
                    Good to know you <set name="username"><star/></set>.
                </li>
            </condition>
        </template>
    </category>

        <category>
        <pattern>MY NAME IS * </pattern>
        <template>
        <think><set name="username"><star/></set></think>
        Nice to know you <get name="username"/>.
        </template>
    </category>





      <category>
      <pattern>WHAT IS * *</pattern>
      <template>

        <get name ="word"></get> : <get name ="description"></get>.
        <think><set name ="word"><star index="2"/></set></think>
      </template>
    </category>

      <category>
      <pattern>WHAT IS *</pattern>
      <template>

         <get name ="word"></get> : <get name ="description"></get>.
         <think><set name ="word"><star/></set></think>
      </template>
    </category>

    <!-- WHO IS pattern moved to handle general queries only after relationship patterns -->
    <category>
      <pattern>WHO IS *</pattern>
      <template>
        <think><set name ="word"><star/></set></think>
        <condition name="description">
            <li value="">I don't know who <star/> is. Could you tell me more about them?</li>
            <li><get name ="word"></get> is <get name ="description"></get>.</li>
        </condition>
      </template>
    </category>

<category>
    <pattern>*</pattern>
    <template>
        <random>
            <li>Unfortunately, I don't know that! How is your mood?</li>
            <li>I'm not sure about that. But tell me, how are you feeling today?</li>
