<!-- Basic YES responses -->
<category>
<pattern>YES</pattern>
<template>
<random>
<li>Great!</li>
<li>Excellent!</li>
<li>I'm glad you agree.</li>
<li>That's wonderful.</li>
<li>Perfect!</li>
<li>Good to hear.</li>
<li>Awesome!</li>
<li>That's good.</li>
</random>
</template>
</category>

<category>
<pattern>YES *</pattern>
<template>
<random>
<li>I understand.</li>
<li>OK, <sr/>.</li>
<li>Great! <sr/>.</li>
<li>That's interesting.</li>
</random>
</template>
</category>

