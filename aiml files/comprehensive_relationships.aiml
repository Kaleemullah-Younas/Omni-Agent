<aiml version="2.0">

<!-- Comprehensive Relationship Patterns -->

<!-- Enhanced Pattern: MY [RELATION] NAME IS [NAME] with duplicate checking -->
<category>
    <pattern>MY FATHER NAME IS *</pattern>
    <template>
        <condition name="father">
            <li value="">
                <think><set name="relation">father</set><set name="person1"><star/></set><set name="father"><star/></set><set name="other_gender">male</set></think>
                Okay, your father's name is <star/>.
            </li>
            <li>
                <think><set name="relation">father</set><set name="person1"><star/></set><set name="other_gender">male</set></think>
                <condition>
                    <li name="father" value="<star/>">I already know that <star/> is your father.</li>
                    <li>I know your father's name already. This name you have given is wrong. Your father name is <get name="father"/>.</li>
                </condition>
            </li>
        </condition>
    </template>
</category>

